# Discord Bot Token (dari Discord Developer Portal)
DISCORD_TOKEN=your_discord_bot_token_here

# Lavalink Configuration
# Untuk Railway: gunakan internal URL dari Lavalink service
# Format: hostname:port (tanpa http://)
LAVALINK_HOST=your-lavalink-service.railway.internal:2333

# Password yang sama dengan yang di-set di Lavalink server
LAVALINK_PASSWORD=youshallnotpass

# Set true jika menggunakan HTTPS/WSS
LAVALINK_SECURE=false
