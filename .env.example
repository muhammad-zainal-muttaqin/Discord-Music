# Discord Bot Token (from Discord Developer Portal)
DISCORD_TOKEN=your_discord_bot_token_here

# Lavalink Configuration
# For Railway: use the internal URL of the Lavalink service
# Format: hostname:port (without http://)
LAVALINK_HOST=your-lavalink-service.railway.internal:2333

# Password must match the one set in Lavalink server
LAVALINK_PASSWORD=youshallnotpass

# Set to true if using HTTPS/WSS
LAVALINK_SECURE=false
